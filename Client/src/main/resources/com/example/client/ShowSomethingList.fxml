<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="530.0" prefWidth="804.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="windowControllers.ShowSomethingList">
   <children>
      <Pane prefHeight="82.0" prefWidth="804.0" style="-fx-background-color: #355326;">
         <children>
            <Button fx:id="backButton" layoutX="26.0" layoutY="24.0" mnemonicParsing="false" onAction="#onBackButtonClick" style="-fx-background-color: #7C9982; -fx-background-radius: 0;" text="Back" textFill="WHITE">
               <font>
                  <Font name="Comic Sans MS" size="20.0" />
               </font>
            </Button>
            <HBox alignment="CENTER" layoutX="164.0" layoutY="22.0" prefHeight="47.0" prefWidth="477.0">
               <children>
                  <Label prefHeight="47.0" prefWidth="114.0" text="List of" textFill="WHITE">
                     <font>
                        <Font name="Comic Sans MS" size="33.0" />
                     </font>
                  </Label>
                  <Label fx:id="somethingLabel" layoutX="10.0" layoutY="10.0" prefHeight="47.0" prefWidth="161.0" text="something" textFill="WHITE">
                     <font>
                        <Font name="Comic Sans MS" size="33.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
         </children>
      </Pane>
      <VBox alignment="TOP_CENTER" layoutX="515.0" layoutY="179.0" prefHeight="313.0" prefWidth="255.0" spacing="10.0">
         <children>
            <Button fx:id="addButton" mnemonicParsing="false" onAction="#onAddButtonClick" style="-fx-background-color: #7C9982; -fx-background-radius: 0;" styleClass="defaultButtonClass" text="Add" textFill="WHITE">
               <font>
                  <Font name="Comic Sans MS" size="25.0" />
               </font>
            </Button>
            <Separator prefWidth="200.0" />
            <Button fx:id="openButton" disable="true" layoutX="12.0" layoutY="24.0" mnemonicParsing="false" onAction="#onShowButtonClick" style="-fx-background-color: #7C9982; -fx-background-radius: 0;" styleClass="defaultButtonClass" text="Show" textFill="WHITE">
               <font>
                  <Font name="Comic Sans MS" size="25.0" />
               </font>
            </Button>
            <Button fx:id="editButton" disable="true" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#onEditButtonClick" style="-fx-background-color: #7C9982; -fx-background-radius: 0;" styleClass="defaultButtonClass" text="Edit" textFill="WHITE">
               <font>
                  <Font name="Comic Sans MS" size="25.0" />
               </font>
            </Button>
            <Button fx:id="deleteButton" disable="true" layoutX="10.0" layoutY="117.0" mnemonicParsing="false" onAction="#onDeleteButtonClick" style="-fx-background-color: #7C9982; -fx-background-radius: 0;" styleClass="defaultButtonClass" text="Delete" textFill="WHITE">
               <font>
                  <Font name="Comic Sans MS" size="25.0" />
               </font>
            </Button>
            <Button fx:id="sureDeleteButton" mnemonicParsing="false" onAction="#onSureDeleteButtonClick" style="-fx-background-color: #7C9982; -fx-background-radius: 0;" styleClass="defaultButtonClass" text="Точно удалить" textFill="WHITE" visible="false">
               <font>
                  <Font name="Comic Sans MS" size="25.0" />
               </font>
            </Button>
         </children>
      </VBox>
      <ListView fx:id="itemList" layoutX="23.0" layoutY="96.0" prefHeight="417.0" prefWidth="464.0" />
   </children>
</AnchorPane>
