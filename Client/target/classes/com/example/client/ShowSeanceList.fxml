<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="455.0" prefWidth="833.0" style="-fx-background-color: #F9F8F6;" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="windowControllers.seances.ShowSeanceList">
   <children>
      <TableView fx:id="table" layoutX="67.0" layoutY="74.0" prefHeight="284.0" prefWidth="699.0">
         <columns>
            <TableColumn fx:id="column_id" prefWidth="119.0" text="Showing number" />
            <TableColumn fx:id="column_movie" prefWidth="156.0" text="Movie" />
            <TableColumn fx:id="column_cinema" prefWidth="126.0" text="Cinema" />
            <TableColumn fx:id="column_date" prefWidth="90.0" text="Date" />
            <TableColumn fx:id="column_time" prefWidth="88.0" text="Time" />
            <TableColumn fx:id="column_price" prefWidth="120.0" text="Cost, from" />
         </columns>
      </TableView>
      <Pane prefHeight="65.0" prefWidth="833.0" style="-fx-background-color: #355326;">
         <children>
            <Label layoutX="324.0" layoutY="9.0" text="Showing list" textFill="WHITE">
               <font>
                  <Font name="Comic Sans MS" size="33.0" />
               </font>
            </Label>
            <Button fx:id="backButton" layoutX="14.0" layoutY="12.0" mnemonicParsing="false" onAction="#onBackButtonClick" style="-fx-background-color: #7C9982; -fx-background-radius: 0;" text="Back" textFill="WHITE">
               <font>
                  <Font name="Comic Sans MS" size="20.0" />
               </font>
            </Button>
         </children>
      </Pane>
      <Button fx:id="addButton" layoutX="40.0" layoutY="377.0" mnemonicParsing="false" onAction="#onAddButtonClick" style="-fx-background-color: #7C9982; -fx-background-radius: 0;" styleClass="defaultButtonClass" text="Add a showing" textFill="WHITE">
         <font>
            <Font name="Comic Sans MS" size="20.0" />
         </font>
      </Button>
      <VBox alignment="CENTER" layoutX="288.0" layoutY="377.0" prefHeight="65.0" prefWidth="258.0" spacing="10.0">
         <children>
            <Button fx:id="showStatsButton" disable="true" layoutX="10.0" layoutY="41.0" mnemonicParsing="false" onAction="#onShowStatsButtonClick" style="-fx-background-color: #7C9982; -fx-background-radius: 0;" styleClass="defaultButtonClass" text="Check grossing" textFill="WHITE">
               <font>
                  <Font name="Comic Sans MS" size="20.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
