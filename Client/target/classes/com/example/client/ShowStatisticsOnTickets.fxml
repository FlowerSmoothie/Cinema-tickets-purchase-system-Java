                  <TableColumn fx:id="column_VIP_bought" prefWidth="75.0" text="bought" />
               </columns>
            </TableColumn>
        </columns>
      </TableView>
      <Button fx:id="showButton" layoutX="316.0" layoutY="368.0" mnemonicParsing="false" onAction="#onShowButtonClick" prefHeight="32.0" prefWidth="118.0" style="-fx-background-color: #7C9982; -fx-background-radius: 0;" text="Show" textFill="WHITE">
         <font>
            <Font name="Comic Sans MS" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
